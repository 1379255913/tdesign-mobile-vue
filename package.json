{
  "name": "tdesign-mobile-vue",
  "purename": "tdesign",
  "version": "0.3.6",
  "description": "tdesign-mobile-vue",
  "author": "TDesign Group",
  "homepage": "http://tdesign.tencent.com/vue-mobile/",
  "bugs": {
    "url": "/tdesign-mobile-vue/issues"
  },
  "repository": {
    "type": "git",
    "url": "/tdesign-mobile-vue.git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "dev:debug": "cross-env NODE_ENV=development vite --debug",
    "prebuild": "rimraf dist/* lib/* es/* esm/* typings/*",
    "build": "cross-env NODE_ENV=production rollup -c scripts/rollup.config.js",
    "site:preview": "vite build --mode staging",
    "build:docs": "vite build",
    "update:icon": "node src/icon/generate/index.js clear && node src/icon/generate/index.js && node src/icon/generate/index.js demo",
    "update:coverage-badge": "",
    "test:unit": "jest --config script/test/jest.unit.conf.js",
    "test:coverage": "jest --config script/test/jest.unit.conf.js --coverage",
    "test:e2e": "cypress run",
    "test:e2e-gui": "cypress open",
    "lint": "eslint --ext .vue,.ts,.tsx ./ --max-warnings 0 --ignore-path .gitignore --ignore-path .eslintignore",
    "lint:fix": "eslint --ext .vue,.ts,.tsx ./ --max-warnings 0 --ignore-path .gitignore --ignore-path .eslintignore --fix",
    "prettier:fix": "prettier ./ --ignore-path .gitignore --ignore-path .eslintignore --write './*.{vue,ts,tsx}' --ignore-unknown",
    "changelog": "publish-cli changelog",
    "robot": "publish-cli robot-msg",
    "commit": "git-cz"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "vue",
    "mobile",
    "tdesign"
  ],
  "title": "tdesign-mobile-vue",
  "types": "dist/index.d.ts",
  "main": "lib/index-lib.js",
  "module": "es/index.js",
  "typings": "typings/src/index.d.ts",
  "unpkg": "dist/tdesign.min.js",
  "jsdelivr": "dist/tdesign.min.js",
  "files": [
    "es",
    "esm",
    "lib",
    "dist",
    "typings",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "sideEffects": [
    "dist/*",
    "docs/**",
    "es/**/style/**",
    "esm/**/style/**"
  ],
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.12.11",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@popperjs/core": "^2.6.0",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-url": "^6.0.0",
    "@soerenmartius/vue3-clipboard": "^0.1.1",
    "@types/lru-cache": "^5.1.0",
    "@types/markdown-it": "^12.0.1",
    "@types/markdown-it-anchor": "^7.0.0",
    "@types/markdown-it-container": "^2.0.3",
    "@types/prismjs": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.1.1",
    "@vue/eslint-config-typescript": "^9.1.0",
    "autoprefixer": "^10.2.4",
    "axios": "^0.21.0",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "debug": "^4.3.1",
    "esbuild": "^0.12.15",
    "eslint": "^8.4.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^6.0.0",
    "jest": "^27.0.0-next.1",
    "less": "^4.0.0",
    "lint-staged": "^10.5.4",
    "lru-cache": "^6.0.0",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^6.0.1",
    "markdown-it-container": "^2.0.0",
    "markdown-it-toc-done-right": "^4.2.0",
    "postcss": "^8.2.1",
    "prettier": "^2.5.1",
    "prismjs": "^1.23.0",
    "qrcode.vue": "^3.2.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.47.0",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-esbuild": "^4.5.0",
    "rollup-plugin-ignore-import": "^1.3.2",
    "rollup-plugin-multi-input": "1.1.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-static-import": "^0.1.1",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "tdesign-icons-vue-next": "^0.0.2",
    "tdesign-publish-cli": "0.0.8",
    "tdesign-site-components": "^0.1.4",
    "typescript": "^4.5.2",
    "vite": "^2.5.0",
    "vite-plugin-tdoc": "^1.0.4",
    "vue": "^3.2.4",
    "vue-eslint-parser": "^8.0.1",
    "vue-router": "4.0.11",
    "vue-type-check": "^1.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
